init_lambda_weights = 0.0 5.08132 9.91933 14.48559 15.60722 16.63293 14.36417 0.0 3.66024 5.87252 6.77443 6.75399 6.14723 2.27285 0.0 3.99495 7.21046 10.09178 10.7052 11.146 8.84515 0.0 -1.90088 -4.25168 -3.77209 -2.53314 0.19038 3.57429 0.0 1.22556 -0.6586 -4.62047 -6.06426 -8.42919 -13.16431
